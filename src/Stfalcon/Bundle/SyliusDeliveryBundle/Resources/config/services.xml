<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="stfalcon_sylius_delivery.in_time.adapter"
                 class="Stfalcon\Bundle\SyliusDeliveryBundle\Adapter\InTimeAdapter">
            <tag name="stfalcon.delivery_adapter" alias="in_time"/>
        </service>
        <service id="stfalcon_sylius_delivery.delivery.adapter"
                 class="Stfalcon\Bundle\SyliusDeliveryBundle\Adapter\DeliveryAdapter">
            <tag name="stfalcon.delivery_adapter" alias="delivery"/>
        </service>
        <service id="stfalcon_sylius.delivery.provider"
                 class="Stfalcon\Bundle\SyliusDeliveryBundle\Provider\DeliveryProvider">
        </service>
    </services>
</container>
